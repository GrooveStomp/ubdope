<section>
  <div class="row">
    <div class="span16">
      <h2>Select Which Sub-Account</h2>

      <div class="well">
        <% @data.each do |account, sub_accounts| %>
          <h3>Account: <%= account %></h3>
          <ul>
          <% sub_accounts.each do |sub| %>
            <li><a href="sub_account/<%= sub['id'] %>"><%= sub['name'] %></a>
          <% end %>
          </ul>
        <% end %>
      </div>

    </div>
  </div>
</section>
